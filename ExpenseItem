import "./ExpenseItem.css"
function ExpenseItem(){
  const expenseDate= new Date(2023, 5, 21)
  const expenseName="Car Expense"
  const expensePrice=300
  const LocationOfExpenditure="Pune"
    return (
      <div className="expenseItem">
         <h2>{expenseDate.toISOString}</h2>
         <div className="expenseItem__items">
          <h2>{expenseName}</h2>
          <div className="expenseItem__price">
            <h2>{expensePrice}</h2>
          </div>
          <div className="LocationOfExpenditure">
            <h2>{LocationOfExpenditure}</h2>
          </div>
         </div>
      </div> 
    );
}

export default ExpenseItem;
